        .dct-border-rht {
            border-right: 1px solid #f0f0f0;
        }

        .dash-widget {
            /* -webkit-box-align: center; */
            -ms-flex-align: center;
            /* align-items: center; */
            /* display: -webkit-box; */
            display: -ms-flexbox;
            display: flex;
        }

        .circle-bar {
            margin-right: 15px;
        }

            .circle-bar > div {
                display: inline-block;
                position: relative;
                text-align: center;
            }

                .circle-bar > div canvas {
                    width: 90px !important;
                    height: 90px !important;
                }

        .btn-group > .btn, .btn-group-vertical > .btn {
            position: unset !important;
            flex: auto;
        }

        .circle-bar > div i {
            left: 0;
            position: absolute;
            top: 50%;
            right: 0;
            text-align: center;
            margin: 0 auto;
            transform: translateY(-50%);
        }

        .pad {
            padding-right: calc(var(--bs-gutter-x)*.2) !important;
            padding-left: calc(var(--bs-gutter-x)*.2) !important;
        }

        .bad {
            transform: translate(125%, 6%);
            float: right;
        }

        .bad1 {
            transform: translate(-123%, 6%);
            float: left;
        }

        .bad3 {
            transform: translate(-127%, 6%);
            float: left;
        }

        .bad2 {
            transform: translate(-123%, 6%);
            float: left;
        }

        .badge {
            /* display: inline-block; */
            padding: 0.52em 0.593em;
            font-size: 0.8125em;
            font-weight: 500;
            line-height: 1;
            color: #fff;
            text-align: center;
            white-space: nowrap;
            vertical-align: baseline;
            border-radius: 1.25rem !important;
            text-transform: none !important;
        }

        .centerbad {
            background-color: #e7e7ff;
            border: 0px !important;
        }

        .leftbad {
            margin-left: 1% !important;
            background-color: #e7e7ff;
            border: 0px !important;
        }

        .rightbad {
            margin-right: 5px !important;
            background-color: #e7e7ff;
            border: 0px !important;
        }


        .custom-option-icon .custom-option-content {
            text-align: center;
            padding: 0.5em !important;
        }

        .avatar-sm {
            width: 1.5rem;
            height: 1.5rem;
            padding-left: 0;
            /* //border: 1px solid #d9dee3;*/
            border-radius: 0rem;
            margin-top: 8px;
        }

        .model {
            margin-top: 23px;
        }

        .custom-option-icon .custom-option-body {
            display: block;
            margin-bottom: 0rem;
        }

        .custom-option-title {
            /* // color:white !important;*/
        }

        .avatar {
            background-color: #e7e7ff;
        }

        .custom-option {
            padding-left: 0;
            border: 0px;
            border-radius: 0rem;
        }

        .remove {
            text-transform: uppercase;
            background-color: #e7e7ff;
            flex-shrink: 0;
            width: 100%;
            max-width: 100%;
            padding-right: 0rem;
            padding-left: 0rem;
            margin-top: var(--bs-gutter-y);
